{
  "name": "affilate-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "babel-node index.js",
    "build-predictive-search": "babel-node scripts/build-search.js",
    "pull-mocks": "NODE_ENV=test babel-node pullAllMocks.js",
    "lint": "prettier --write '**/*.js'",
    "test": "cross-env NODE_ENV=test PREVENT_LOGS=true jest --collect-coverage",
    "test:integration": "cross-env NODE_ENV=test npm run pull-mocks && npm run test",
    "test-e2e": "babel-node scripts/testMockCycleIndex.js",
    "full-test-suite": "npm run pull-mocks && npm run test && npm run test-e2e",
    "test-core-api": "babel-node scripts/testCoreApi.js",
    "test-slack-api": "babel-node scripts/testSlackApi.js",
    "test-store-cache": "babel-node scripts/testWriteStoreCache.js",
    "test-batch": "babel-node scripts/testBatch.js",
    "log-store-cache": "babel-node scripts/logStoreCache.js",
    "update-tests": "npm test -- -u"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "chrome-aws-lambda": "^2.1.1",
    "compress-json": "^1.0.1",
    "compressed-json": "^1.0.15",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "flow-bin": "^0.115.0",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "jsoncomp": "^1.6.1",
    "jsoncompress": "^0.1.3",
    "jsonpack": "^1.1.5",
    "lodash": "^4.17.20",
    "modern-random-ua": "^1.0.3",
    "moment": "^2.29.1",
    "object-sizeof": "^1.6.1",
    "pretty-ms": "^7.0.0",
    "puppeteer": "^5.2.1",
    "puppeteer-extra": "^3.1.9",
    "puppeteer-extra-plugin-aws": "^1.0.2",
    "puppeteer-extra-plugin-stealth": "^2.5.0",
    "regenerator-runtime": "^0.13.3",
    "require-context": "^1.1.0",
    "sizeof": "^1.0.0",
    "stopwatch-node": "^1.0.2",
    "zipson": "^0.2.12"
  },
  "devDependencies": {
    "@babel/runtime": "^7.11.2",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-flow": "^7.10.4",
    "@reactioncommerce/components-context": "^1.2.0",
    "@types/node": "^13.13.15",
    "ansi-regex": "^5.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "chalk": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
      "jest": "^24.8.0",
    "jest-serializer-html-string": "^1.0.1",
    "nodemon": "^1.19.1"
  },
  "jest": {
    "verbose": false,
    "setupFiles": [
      "./jest-utils/jestGlobals.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@lib": "<rootDir>/src/lib/index",
      "^@helpers": "<rootDir>/src/helpers/index",
      "^@experiments": "<rootDir>/src/experiments/index",
      "^@polyfills": "<rootDir>/src/_bespoke/index"
    }
  }
}
