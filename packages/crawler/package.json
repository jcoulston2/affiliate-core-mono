{
  "name": "@affiliate-master/crawler",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start-crawl": "FORCE_COLOR=1 babel-node index.js",
    "pull-page-mocks": "NODE_ENV=test babel-node pullAllMocks.js",
    "lint": "prettier --write '**/*.js'",
    "test": "cross-env NODE_ENV=test PREVENT_LOGS=true jest --collect-coverage",
    "test:integration": "cross-env NODE_ENV=test npm run pull-mocks && npm run test",
    "test-e2e": "babel-node scripts/testMockCycleIndex.js",
    "full-test-suite": "npm run pull-mocks && npm run test && npm run test-e2e",
    "test-slack-api": "babel-node scripts/testSlackApi.js",
    "test-store-cache": "babel-node scripts/testWriteStoreCache.js",
    "log-store-cache": "babel-node scripts/logStoreCache.js",
    "update-tests": "npm test -- -u"
  },
  "dependencies": {
    "@affiliate-master/common": "^1.0.0",
    "@affiliate-master/config": "^1.0.0",     
    "@affiliate-master/store": "^1.0.0",    
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "compress-json": "^1.0.1",
    "compressed-json": "^1.0.15",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "flow-bin": "^0.115.0",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "jsoncomp": "^1.6.1",
    "jsoncompress": "^0.1.3",
    "jsonpack": "^1.1.5",
    "lodash": "^4.17.20",
    "modern-random-ua": "^1.0.3",
    "moment": "^2.29.1",
    "object-sizeof": "^1.6.1",
    "pretty-ms": "^7.0.0",
    "puppeteer": "^10.1.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-stealth": "^2.7.8",
    "regenerator-runtime": "^0.13.3",
    "require-context": "^1.1.0",
    "sizeof": "^1.0.0",
    "stopwatch-node": "^1.0.2",
    "zipson": "^0.2.12"
  },
  "devDependencies": {
    "ansi-regex": "^5.0.0",
    "chalk": "^3.0.0",
    "cross-env": "^7.0.2"
  },
  "jest": {
    "verbose": false,
    "setupFiles": [
      "./jest-utils/jestGlobals.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@lib": "<rootDir>/src/lib/index",
      "^@helpers": "<rootDir>/src/helpers/index",
      "^@experiments": "<rootDir>/src/experiments/index",
      "^@polyfills": "<rootDir>/src/_bespoke/index"
    }
  }
}
