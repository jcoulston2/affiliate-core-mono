{
  "extracts": {
    "topLevel": {
      "scrollPage": true,
      "scrollPageWaitTime": 15000,
      "waitForNode": null,
      "delay": 0,
      "productsSelector": {
        "selector": ".ProductList .Product"
      },
      "data": {
        "name": {
          "selector": ".Product-name a",
          "getDataFrom": "text"
        },
        "price": {
          "selector": ".HistoricalPrice .Price",
          "getDataFrom": "text"
        },
        "wasPrice": {
          "selector": ".HistoricalPrice-was .Price",
          "getDataFrom": "text",
          "isConditional": true
        },
        "nowPrice": {
          "selector": ".HistoricalPrice-promotion .Price",
          "getDataFrom": "text",
          "isConditional": true
        },
        "link": {
          "selector": ".Product-link",
          "getDataFrom": "attr: href"
        },
        "image": {
          "selector": ".ProductImages-image",
          "getDataFrom": "attr: src"
        }
      }
    },
    "details": {
      "data": {
        "images": {
          "selector": ".ProductDetail .Carousel-image",
          "getDataFrom": "attr: src"
        }
      }
    }
  }
}
